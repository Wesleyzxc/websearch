{
  "name": "websearch",
  "displayName": "WebSearch",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.56.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:websearch.searchSelected",
    "onCommand:websearch.searchWeb",
    "onCommand:websearch.configureViewOnWindowOpen"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "websearch.searchSelected",
        "title": "Web Search: Search Selected Text"
      },
      {
        "command": "websearch.searchWeb",
        "title": "Web Search: Search The Web"
      },
      {
        "command": "websearch.configureViewOnWindowOpen",
        "title": "Web Search: Change Search Engine"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "when": "editorHasSelection",
          "command": "websearch.searchSelected",
          "group": "navigation"
        }
      ]
    },
    "configuration": {
      "title": "Web Search Config",
      "type": "object",
      "properties": {
        "websearch.QueryTemplate": {
          "type": "string",
          "default": "https://www.google.com/search?q=",
          "description": "Search Query"
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.1.3",
    "@types/mocha": "^8.0.4",
    "@types/node": "^12.11.7",
    "@types/vscode": "^1.56.0",
    "@typescript-eslint/eslint-plugin": "^4.14.1",
    "@typescript-eslint/parser": "^4.14.1",
    "eslint": "^7.19.0",
    "glob": "^7.1.6",
    "mocha": "^8.2.1",
    "typescript": "^4.1.3",
    "vscode-test": "^1.5.0"
  }
}
